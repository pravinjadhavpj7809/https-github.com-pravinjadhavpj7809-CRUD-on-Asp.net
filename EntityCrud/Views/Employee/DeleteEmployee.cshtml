        </tr>
        <tr>
            <td>Salary</td>
            <td>@Html.TextBoxFor(E => E.salary, new { @readonly = true })</td>
        </tr>
        <tr>
            <td>Did</td>
            <td>@Html.TextBoxFor(E => E.Did, new { @readonly = true })</td>
        </tr>
