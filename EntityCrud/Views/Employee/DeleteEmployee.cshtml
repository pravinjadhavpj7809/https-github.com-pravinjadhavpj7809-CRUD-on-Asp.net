@model EntityCrud.Models.Employee

@{
    ViewBag.Title = "DeleteEmployee";
}

<h2>DeleteEmployee</h2>

@using (Html.BeginForm("DeleteEmployee", "Employee"))
{
    <table border="1">
        <tr>
            <td>Eid</td>
            <td>@Html.TextBoxFor(E => E.Eid, new { @readonly = true })</td>
        </tr>
        <tr>
            <td>Ename</td>
            <td>@Html.TextBoxFor(E => E.Ename, new { @readonly = true })</td>
        </tr>
